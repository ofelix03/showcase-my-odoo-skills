<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="payment_plan_sequence">
            <field name="name">Payment Plan Number</field>
            <field name="code">payment.plan.number</field>
            <field name="active">TRUE</field>
            <field name="prefix">PP/%(year)s%(month)s%(day)s/</field>
